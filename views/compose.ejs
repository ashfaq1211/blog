
<%- include("partials/header") -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" name="titleText" class="form-control">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea name="postText" class="form-control" rows="5" cols="80"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>
